apiVersion: v1
kind: ConfigMap
metadata:
  name: grafana-datasource
  labels:
    
    app.kubernetes.io/name: cost-analyzer
    helm.sh/chart: cost-analyzer-1.41.1
    app.kubernetes.io/instance: limping-condor
    app.kubernetes.io/managed-by: Tiller
    app: cost-analyzer
    grafana_datasource: "1"
data:
  datasource.yaml: |-
    # config file version
    apiVersion: 1
    datasources:
    - access: proxy
      isDefault: true
      name: Prometheus
      type: prometheus
      url: http://limping-condor-prometheus-server.default.svc.cluster.local
---
