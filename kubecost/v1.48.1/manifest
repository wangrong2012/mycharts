# Source: cost-analyzer/templates/cost-analyzer-postgres-deployment.yaml
apiVersion: extensions/v1beta1
kind: Deployment
metadata:
  name: limping-condor-cost-analyzer-postgres
  labels:
    
    app.kubernetes.io/name: cost-analyzer
    helm.sh/chart: cost-analyzer-1.41.1
    app.kubernetes.io/instance: limping-condor
    app.kubernetes.io/managed-by: Tiller
    app: cost-analyzer
spec:
  strategy:
    rollingUpdate:
      maxSurge: 1
      maxUnavailable: 1
    type: RollingUpdate
  template:
    metadata:
      labels:
        app: postgres
    spec:
        containers:
            - image: timescale/pg_prometheus:latest
              name: pgprometheus
              ports:
              - containerPort: 5432
              args:
                - -csynchronous_commit=off
              env:
                - name: POSTGRES_PASSWORD
                  value: admin
                - name: POSTGRES_USER
                  value: postgres
              volumeMounts:
                - mountPath: /var/lib/postgresql/data
                  name: postgres-pv-volume
                  subPath: postgres
        volumes:
          - name: postgres-pv-volume
            persistentVolumeClaim:
              claimName: postgres-pv-claim
---
# Source: cost-analyzer/templates/cost-analyzer-prometheus-postgres-adapter-deployment.yaml
apiVersion: extensions/v1beta1
kind: Deployment
metadata:
  name: limping-condor-cost-analyzer-adapter
  labels:
    
    app.kubernetes.io/name: cost-analyzer
    helm.sh/chart: cost-analyzer-1.41.1
    app.kubernetes.io/instance: limping-condor
    app.kubernetes.io/managed-by: Tiller
    app: cost-analyzer
spec:
  strategy:
    rollingUpdate:
      maxSurge: 1
      maxUnavailable: 1
    type: RollingUpdate
  template:
    metadata:
      labels:
        app: adapter
    spec:
      initContainers:
        - name: kubecost-sql-init
          image: ajaytripathy/kubecost-sql-init:prod-1.41.1
          imagePullPolicy: Always
          env:
            - name: PROMETHEUS_SERVER_ENDPOINT
              valueFrom:
                configMapKeyRef:
                  name: limping-condor-cost-analyzer
                  key: prometheus-server-endpoint
      containers:
          - image: timescale/prometheus-postgresql-adapter:latest
            name: pgprometheusadapter
            ports:
              - containerPort: 9201
            args:
              - "-pg-host=pgprometheus"
              - "-pg-prometheus-log-samples=true"
              - "-pg-password=admin"
---
# Source: cost-analyzer/templates/cost-analyzer-checks-template.yaml
apiVersion: batch/v1beta1
kind: CronJob
metadata:
  name: cost-analyzer-checks
  labels:
    
    app.kubernetes.io/name: cost-analyzer
    helm.sh/chart: cost-analyzer-1.41.1
    app.kubernetes.io/instance: limping-condor
    app.kubernetes.io/managed-by: Tiller
    app: cost-analyzer
spec:
  schedule: "*/10 * * * *"
  jobTemplate:
    spec:
      template:
        spec:
          containers:
          - name: cost-analyzer-checks
            image: ajaytripathy/kubecost-checks:prod-1.41.1
            args:
            - node
            - ./node/cron.js
            resources:
              requests:
                cpu: "20m"
                memory: "100Mi"
              limits:
                cpu: "100m"
                memory: "200Mi"
            env:
            - name: COST_ANALYZER_SERVER_ENDPOINT
              value: limping-condor-cost-analyzer.default.svc.cluster.local:9001
            - name: COST_ANALYZER_MODEL_ENDPOINT
              value: limping-condor-cost-analyzer.default.svc.cluster.local:9003
            - name: PROMETHEUS_ALERTMANAGER_ENDPOINT
              valueFrom:
                configMapKeyRef:
                  name: limping-condor-cost-analyzer
                  key: prometheus-alertmanager-endpoint
          restartPolicy: OnFailure
